public class examA {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int number = n;
		int sum = 0;
		
		while (number > 0 ) {
			sum += number %10; number /= 10;
		}
		
		System.out.println(sum);

		

	}

}
