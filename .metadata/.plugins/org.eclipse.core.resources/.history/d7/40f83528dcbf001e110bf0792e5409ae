package test;

import java.util.Scanner;

class Exam3 {
	public int solution () {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int number = n;
		int sum = 0;
		
		while (number > 0 ) {
			sum += number %10; number /= 10;
		}
		System.out.println(sum);
		return 0;
	}
	
}

public class exam {

	public static void main(String[] args) {

		Exam3 instance3 = new Exam3();
		instance3.solution();
		
	}

}
