package test;

import java.util.List;
import java.util.Scanner;

class exam1 {
	public int solution (int[] numList) {
		int answer = -1;
		    for(int i = 0; i < numList.length; i++){
		        if(numList[i] < 0){
		            answer = i;
		            break;
		        }
		    } return answer;	 
	}
}


